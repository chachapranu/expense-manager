{
  "cli": {
    "version": ">= 3.0.0"
  },
  "build": {
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "local": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "enableProguardInReleaseBuilds": true
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle",
        "enableProguardInReleaseBuilds": true
      }
    }
  }
}
